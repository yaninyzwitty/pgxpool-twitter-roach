type User {
  id: ID!
  username: String!
  email: String!
  createdAt: Time! # Custom scalar, map to google.protobuf.Timestamp
}

scalar Time

type Query {
  getUserById(id: ID!): User!
  getUserByEmail(email: String!): User!
  getUsers(limit: Int = 10, offset: Int = 0): [User!]!
}

type Subscription {
  streamUsers(limit: Int = 100): User!
}
